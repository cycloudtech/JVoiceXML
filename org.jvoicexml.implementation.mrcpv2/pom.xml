<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>
	<groupId>net.sourceforge.halef</groupId>
	<artifactId>doesnotapply</artifactId>
	<packaging>jar</packaging>
	<name>This is just to get/pull Cairo jar dependencies for JVXML from a Maven repo</name>
	<!--  version>${cairo-version}</version -->
	<version>0.1-SNAPSHOT</version>
	


	<build>

		<directory>target</directory>
		<outputDirectory>target/classes</outputDirectory>
		<finalName>${artifactId}-${version}</finalName>
		<testOutputDirectory>target/test-classes</testOutputDirectory>
		<sourceDirectory>src/main/java</sourceDirectory>
		<scriptSourceDirectory>src/main/scripts</scriptSourceDirectory>
		<testSourceDirectory>src/test/java</testSourceDirectory>

		<plugins>


			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-dependency-plugin</artifactId>
				<version>2.10</version>
				<executions>
					<execution>
						<id>copy</id>
						<phase>initialize</phase>
						<goals>
							<goal>copy</goal>
						</goals>
						<configuration>
							<artifactItems>
								<artifactItem>
									<groupId>net.sourceforge.halef</groupId>
									<artifactId>cairo-client</artifactId>
									<version>${version}</version>
									<type>jar</type>
									<overWrite>false</overWrite>
									<!-- outputDirectory>${project.build.directory}/alternateLocation</outputDirectory-->
									<destFileName>cairo-client-SNAPSHOT.jar</destFileName>
								</artifactItem>
								<artifactItem>
									<groupId>net.sourceforge.halef</groupId>
									<artifactId>cairo-sip</artifactId>
									<version>${version}</version>
									<type>jar</type>
									<overWrite>false</overWrite>
									<!-- outputDirectory>${project.build.directory}/alternateLocation</outputDirectory-->
									<destFileName>cairo-sip-SNAPSHOT.jar</destFileName>
								</artifactItem>
								<artifactItem>
									<groupId>net.sourceforge.halef</groupId>
									<artifactId>cairo-rtp</artifactId>
									<version>${version}</version>
									<type>jar</type>
									<overWrite>false</overWrite>
									<!-- outputDirectory>${project.build.directory}/alternateLocation</outputDirectory-->
									<destFileName>cairo-rtp-SNAPSHOT.jar</destFileName>
								</artifactItem>
							</artifactItems>
							<outputDirectory>${basedir}/3rdparty/cairo/lib</outputDirectory>
							<overWriteReleases>true</overWriteReleases>
							<overWriteSnapshots>true</overWriteSnapshots>
							<overWriteIfNewer>true</overWriteIfNewer>
						</configuration>
					</execution>
				</executions>
			</plugin>

		</plugins>

	</build>

	<repositories>

		<repository>
			<id>repository.oss.sonatype.org</id>
			<name>oss.sonatype.org</name>
			<url>https://oss.sonatype.org/content/repositories/snapshots/</url>
			<!-- url>https://oss.sonatype.org/content/repositories/snapshots</url -->
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>

		<!-- repository> <id>central</id> <name>Maven Repository Switchboard</name> 
			<layout>default</layout> <url>http://repo1.maven.org/maven2</url> <snapshots> 
			<enabled>false</enabled> </snapshots> </repository -->

	</repositories>

	<dependencies>

		<!-- compile scope dependencies -->

		<dependency>
			<groupId>net.sourceforge.halef</groupId>
			<artifactId>cairo-client</artifactId>
			<version>${version}</version>
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>net.sourceforge.halef</groupId>
			<artifactId>cairo-sip</artifactId>
			<version>${version}</version>
			<scope>compile</scope>
		</dependency>

		<dependency>
			<groupId>net.sourceforge.halef</groupId>
			<artifactId>cairo-rtp</artifactId>
			<version>${version}</version>
			<scope>compile</scope>
		</dependency>

	</dependencies>

	<!-- properties> <project.group.id>149085</project.group.id> </properties -->

</project>
